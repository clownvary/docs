{
	"gitbook": "3.x.x",
	"plugins": [
		"-lunr", 
		"-search",
		"-sharing",
		"back-to-top-button",
		"chapter-fold",
		"copy-code-button",
		"search-pro",
		"splitter",
		"tbfed-pagefooter",
		"page-toc-button",
		"auto-scroll-table",
		"popup",
		"donate",
		"github-buttons",
		"advanced-emoji",
		"sharing-plus"
	],
	"pluginsConfig": {
		"tbfed-pagefooter": {
            "copyright":"Copyright © <a href='https://github.com/clownvary' target='_blank'>garyw</a> 2020",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
		},
		"page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
		   },
		"donate": {
            "wechat": "http://ww1.sinaimg.cn/large/77775fbbly1ge43xiripnj20j90ixjtf.jpg",
            "alipay": "http://ww1.sinaimg.cn/large/77775fbbly1ge43yg7eycj20ah0ai3ze.jpg",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
		},
		"github-buttons": {
			"buttons": [{
			  "user": "clownvary",
			  "repo": "docs",
			  "type": "star",
			  "size": "medium"
			}]
		},
		"sharing": {
			"douban": false,
			"facebook": true,
			"google": true,
			"pocket": false,
			"qq": false,
			"qzone": false,
			"twitter": true,
			"weibo": true,
		 "all": [
			  "douban", "facebook", "google", "instapaper", "linkedin","twitter", "weibo", 
			  "messenger","qq", "qzone","viber","whatsapp"
		  ]
	  }
    },
	"pdf": {
		"pageNumbers": true,
		"fontFamily": "Arial",
		"fontSize": 12,
		"paperSize": "a4",
		"margin": {
			"right": 62,
			"left": 62,
			"top": 56,
			"bottom": 56
		}
	},
	"title": "clown's doc",
	"description": "记录个人相关笔记",
	"author": "gary wang",
	"language": "zh-hans",
	"root": ".",
	"structure": {
		"readme": "introduction.md"
	},
	"links": {
		"sidebar": {
		}
	}
}