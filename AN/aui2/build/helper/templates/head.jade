- var opts = htmlWebpackPlugin.options;
- var diff = opts.diff;
- var linkExtraPrefix = opts.linkExtraPrefix;
- var finalOrgSiteRootURL = "/${orgSiteName}";
meta(charset="UTF-8")
meta(http-equiv="X-UA-Compatible" content="IE=edge")
meta(name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1")
meta(name="format-detection" content="telephone=no")
title=opts.title
if diff !== "static"
  each val, index in htmlWebpackPlugin.files.chunks
    link(href!= finalOrgSiteRootURL + "/" + linkExtraPrefix + val.css rel="stylesheet")

  script.
    function parseObject(obj) {
      if (obj !== undefined) return obj;
      return {};
    }

    function parseArray(arr) {
      if (Array.isArray(arr)) return arr;
      return [];
    }

    function parseBoolean(b) {
      return !!b;
    }

    function parseInteger(i) {
      return parseInt(i || 0);
    }

    function parseNumber(f) {
      return parseFloat(f || 0);
    }

style.
  body {
   background-color: #ffffff !important;
   background-image: ${trainer_background_img};
   background-repeat: repeat;
  }

  .an-page-loading{
    position: fixed;
    top: 0;
    left:0;
    right: 0;
    bottom: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    font-family: Arial, sans-serif;
    font-size: 16px;
    font-weight: 400;
    line-height: 20px;
    box-sizing: border-box;
  }

  .an-page-loading .icon-loading{
    width: 60px;
    height: 60px;
    transform-origin: 25px 31px;
    animation: loading-rotate 2s infinite linear;
  }

  .an-page-loading .text{
    margin-top: 16px;
    font-size: 40px;
    color: #aaa;
  }

  @keyframes loading-rotate {
    to {
      transform: rotate(360deg);
    }
  }
