{
  "type": "object",
  "properties": {
    "headers": {
      "$ref": "headers_success_list"
    },
    "body": {
      "type": "object",
      "properties": {
        "permit_grid_items": {
          "type": "array",
          "minItems": 10,
          "maxItems": 50,
          "uniqueItems": true,
          "items": {
            "type": "object",
            "properties": {
              "permit_id": {
                "type": "integer",
                "minimum": 1,
                "maximum": 100
              },
              "permit_number": {
                "type": "integer"
              },
              "site_name": {
                "type": "string"
              },
              "customer_name": {
                "type": "string",
                "faker": "name.findName"
              },
              "event_name": {
                "type": "string",
                "faker": "name.findName"
              },
              "event_number": {
                "type": "integer",
                "minimum": 1,
                "maximum": 10
              },
              "booking_number": {
                "type": "integer",
                "minimum": 1,
                "maximum": 10
              },
              "permit_status": {
                "$ref": "status_name"
              },
              "status_id": {
                "type": "integer",
                "minimum": 1,
                "maximum": 8
              },
              "permit_start_date": {
                "type": "string",
                "faker": "date.past"
              },
              "permit_end_date": {
                "type": "string",
                "chance": "date"
              }
            },
            "required": [
              "permit_id",
              "permit_number",
              "site_name",
              "customer_name",
              "event_name",
              "event_number",
              "booking_number",
              "permit_status",
              "status_id",
              "permit_start_date",
              "permit_end_date"
            ]
          }
        }
      },
      "required": [
        "permit_grid_items"
      ]
    }
  },
  "required": [
    "headers",
    "body"
  ]
}
