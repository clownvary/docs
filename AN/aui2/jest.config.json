{
  "browser": true,
  "testEnvironment": "jsdom",
  "cacheDirectory": "./node_modules/.cache",
  "verbose": true,
  "globals": {
    "__STATIC__": true,
    "__DEV__": false,
    "__TESTING__": true,
    "__TESTINGHOST__": "localhost",
    "__TESTINGPORT__": 3332
  },
  "transformIgnorePatterns": [
    "/node_modules/(?!react-base-ui).+\\.js$"
  ],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
    "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js",
    "^imports": "<rootDir>/test/__mocks__/jestImportsMock.js"
  },
  "testRegex": "(\\.(test|spec))\\.(jsx|js)$",
  "moduleFileExtensions": [
    "json",
    "jsx",
    "js"
  ],
  "moduleDirectories": [
    "node_modules",
    "src",
    "test"
  ],
  "collectCoverage": false,
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!**/__mocks__/**"
  ],
  "coverageReporters": [
    "json-summary",
    "text",
    "html",
    "cobertura",
    "lcov"
  ],
  "coverageDirectory": "test/coverage",
  "coveragePathIgnorePatterns": [
    "src/shared/validate",
    "src/shared/components/Toast/",
    "src/index/Resource/",
    "app.js",
    "store.js",
    "MagTekCabMSR.js",
    "MagTekUserMSR.js",
    "MagTekCabIPAD.js",
    "MagTekUserIPAD.js",
    "applet.jsx",
    "APDInterface_new_design.js",
    "APDInterface.js",
    "ams-security-api.js",
    "magtekFun.js",
    "utils/Validation.js",
    "urls/index.js",
    "automationIds.js",
    "url_dev.js",
    "url_prod.js",
    "rules/*.*",
    "demo.jsx"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 95,
      "functions": 96,
      "lines": 97,
      "statements": 97
    }
  },
  "setupFiles": [
    "./test/tests.initialState.jest.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/index/Resource/",
    "/examples/",
    "/dist/",
    "/etc/",
    "/json-schema/",
    "/scripts/",
    "/tasks/"
  ],
  "roots": [
    "test/",
    "src/"
  ]
}
