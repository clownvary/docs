{
  "name": "react-base-ui",
  "version": "1.4.55",
  "description": "Active Sharable Library for React Projects.",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:cjs",
    "build:cjs": "better-npm-run build-cjs",
    "build:es": "better-npm-run build-es",
    "clean": "rimraf lib es dist",
    "eslint": "eslint --ext .js,.jsx src",
    "eslint:checkstyle": "eslint --ext .js,.jsx ./src --quiet --format checkstyle > eslint-checkstyle.xml",
    "eslint:fix": "eslint --fix --ext .js,.jsx src",
    "stylelint": "stylelint 'src/**/*.less'",
    "stylelint:fix": "stylelint 'src/**/*.less' --fix",
    "test": "jest --config=jest.config.json --coverage",
    "test:watch": "jest --config=jest.config.json --watch",
    "test:update": "jest --config=jest.config.json --updateSnapshot",
    "release": "better-npm-run release",
    "release:pre": "better-npm-run release:pre",
    "play": "better-npm-run play",
    "play:ip": "better-npm-run play:ip",
    "play:filter": "better-npm-run play:filter",
    "samples": "better-npm-run samples",
    "samples:ip": "better-npm-run samples:ip",
    "site:build": "better-npm-run site:build",
    "site": "npm run site:build && cp -rf ./_site_examples $JENKINS_HOME/userContent",
    "doc": "jsdoc2md 'src/**/*[.jsx|.js]' > doc/api.md",
    "doc:api": "babel-node build/doc-api",
    "gen": "babel-node build/gen",
    "lint": "npm run eslint && npm run stylelint"
  },
  "betterScripts": {
    "site:build": {
      "command": "webpack --config build/webpack.config.examples.babel.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "cjs",
        "DEBUG": "react-base-ui:*",
        "SITE": true
      }
    },
    "build-cjs": {
      "command": "babel ./src -d lib --copy-files",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "cjs",
        "DEBUG": "react-base-ui:*",
        "STATIC": true
      }
    },
    "build-es": {
      "command": "babel ./src -d es --copy-files",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "es",
        "DEBUG": "react-base-ui:*",
        "STATIC": true
      }
    },
    "release": {
      "command": "babel-node build/release",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "react-base-ui:*"
      }
    },
    "release:pre": {
      "command": "babel-node build/release --auto --pre",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "react-base-ui:*"
      }
    },
    "play": {
      "command": "babel-node build/server",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "react-base-ui:*",
        "SANDBOX": true
      }
    },
    "play:ip": {
      "command": "babel-node build/server",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "react-base-ui:*",
        "SANDBOX": true,
        "RUN_BY_IP": "YES"
      }
    },
    "play:filter": {
      "command": "babel-node build/server --p",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "react-base-ui:*",
        "SANDBOX": true,
        "RUN_BY_IP": "YES"
      }
    },
    "samples": {
      "command": "babel-node build/server",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "react-base-ui:*"
      }
    },
    "samples:ip": {
      "command": "babel-node build/server",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "react-base-ui:*",
        "RUN_BY_IP": "YES"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.dev.activenetwork.com:ActiveNet/react-base-ui.git"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "devDependencies": {
    "active.css": "git+ssh://git@gitlab.dev.activenetwork.com:fee/active.css.git#v0.8.74",
