<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv='Content-type' content='text/html; charset=utf-8'/>
    <title><%= htmlWebpackPlugin.options.title || 'react aaui'%></title>
  </head>
  <body>
    <% for (var index in htmlWebpackPlugin.options.entry) { %>
      <p><a href="<%= index%>"><%= index[0].toUpperCase() + index.substring(1) %></a></p>
    <% } %>
  </body>
</html>
