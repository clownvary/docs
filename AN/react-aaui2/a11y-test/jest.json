{
  "bail": true,
  "testRegex": "a11y-test/test/\\w*.js",
  "moduleFileExtensions": [
    "",
    "json",
    "js",
    "jsx",
    "less"
  ],
  "modulePaths": [
    "<rootDir>/a11y-test/test"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "<rootDir>/build/jestStyleMock.js"
  },
  "setupFiles": [
    "<rootDir>/a11y-test/setEnv"
  ],
  "setupTestFrameworkScriptFile": "<rootDir>/a11y-test/setJest"
}
