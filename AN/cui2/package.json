{
  "name": "AN-CUI",
  "version": "1.0.0",
  "description": "New front-end architecture of ActiveNet CUI",
  "scripts": {
    "start": "better-npm-run build:i18n && better-npm-run start",
    "start:ip": "better-npm-run build:i18n && better-npm-run start:ip",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:i18n": "better-npm-run build:i18n",
    "build:dev:stats": "better-npm-run build:dev:stats",
    "build:prod:stats": "better-npm-run build:prod:stats",
    "eslint": "eslint --ext .js,.jsx ./src",
    "eslint:fix": "eslint --fix --ext .js,.jsx ./src",
    "eslint:checkstyle": "eslint --ext .js,.jsx ./src --quiet --format checkstyle > eslint-checkstyle.xml",
    "stylelint": "stylelint 'src/**/*.less'",
    "stylelint:fix": "stylelint 'src/**/*.less' --fix",
    "test": "better-npm-run test",
    "test:watch": "better-npm-run test:watch",
    "test:server": "better-npm-run test:server",
    "jest": "jest --config=jest.config.json",
    "jest:watch": "jest --watch --config=jest.config.json"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config build/webpack.config.static.babel.js",
      "env": {
        "NODE_ENV": "static",
        "DEBUG": "anet-cui:*"
      }
    },
    "start:ip": {
      "command": "webpack-dev-server --config build/webpack.config.static.babel.js",
      "env": {
        "NODE_ENV": "static",
        "DEBUG": "anet-cui:*",
        "RUN_BY_IP": "YES"
      }
    },
    "build:dev": {
      "command": "webpack --config ./build/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "anet-cui:*"
      }
    },
    "build:prod": {
      "command": "webpack --config ./build/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "anet-cui:*"
      }
    },
    "build:i18n": {
      "command": "babel-node build/i18n/index.js",
      "env": {
        "NODE_ENV": "i18n",
        "DEBUG": "anet-cui:*"
      }
    },
    "build:dev:stats": {
      "command": "webpack --config ./build/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "anet-cui:*",
        "STATS": "YES"
      }
    },
    "build:prod:stats": {
      "command": "webpack --config ./build/webpack.config.babel.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "anet-cui:*",
        "STATS": "YES"
      }
    },
    "test": {
      "command": "babel-node ./build/server/index.js",
      "env": {
        "NODE_ENV": "test",
        "HOST": "localhost",
        "PORT": "3337",
        "SCRIPT": "jest",
        "SCRIPTARGS": "--debug,--coverage,--config=jest.config.json"
      }
    },
    "test:watch": {
      "command": "babel-node ./build/server/index.js",
      "env": {
        "NODE_ENV": "test",
        "HOST": "localhost",
        "PORT": "3337",
        "SCRIPT": "jest",
        "SCRIPTARGS": "--debug,--watch,--coverage,--config=jest.config.json"
      }
    },
    "test:server": {
      "command": "babel-node ./build/server/index.js",
      "env": {
        "NODE_ENV": "test",
        "HOST": "localhost",
        "PORT": "3337"
      }
    }
  },
  "repository": "git@gitlab.dev.activenetwork.com:ActiveNet/cui.git",
  "author": "ActiveNetFEEChina@activenetwork.com",
  "license": "UNLICENSED",
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.4.5",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.24.1",
    "better-npm-run": "0.0.15",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "debug": "^2.6.8",
    "enzyme": "^2.8.2",
    "es6-promise": "^3.1.2",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-an": "git+ssh://git@gitlab.dev.activenetwork.com:ActiveNet/eslint-config-an.git#v1.0.15",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "^0.8.5",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jest": "^20.0.3",
    "jsdom": "^11.0.0",
    "json-loader": "0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "parse-filepath": "^1.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-loader": "^0.8.1",
    "react-hot-loader": "3.0.0-beta.7",
    "react-test-renderer": "^15.5.4",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-mock-store": "1.2.3",
    "sinon": "^2.3.1",
    "sinon-chai": "^2.10.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-require-from": "^1.3.1",
    "xmlhttprequest": "1.7.0",
    "yargs": "^8.0.1",
    "stylelint": "~9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^2.0.0"
  },
  "dependencies": {
    "active.css": "git+ssh://git@gitlab.dev.activenetwork.com:fee/active.css.git#v0.8.52",
    "fontfaceobserver": "^2.0.1",
    "history": "3.3.0",
    "jquery": "^3.2.1",
    "radium": "0.18.1",
    "react-base-ui": "git+ssh://git@gitlab.dev.activenetwork.com:ActiveNet/react-base-ui.git#v1.3.54-r.54",
    "react-router-scroll": "^0.4.4",
    "scroll-behavior": "^0.9.5",
    "svg-sprite-loader": "^3.7.1"
  }
}
